{
    "manifest_version": 3,
    "default_locale": "en",
    "name": "Wixi Extension",
    "description": "This Safari Extension allows Wixi to get information about your activity on the web.",
    "version": "1.0.0",
    "browser_action": {
        "default_title": "Wixi Extension",
        "default_icon": {
            "16": "icons/toolbar-16.png",
            "19": "icons/toolbar-19.png",
            "32": "icons/toolbar-32.png",
            "38": "icons/toolbar-38.png"
        }
    },
    "content_scripts": [
        {
            "js": [
                "script.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "icons": {
        "48": "icons/48.png",
        "64": "icons/64.png",
        "96": "icons/96.png",
        "128": "icons/128.png",
        "256": "icons/256.png",
        "512": "icons/512.png"
    }
}